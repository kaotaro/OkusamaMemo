<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical" android:background="@drawable/top_land">

	<LinearLayout
		android:id="@+id/LinearLayout02"
		android:layout_width="fill_parent"
		android:orientation="horizontal"
		android:layout_above="@+id/MenuButtonLayout"
		android:layout_alignParentTop="true"
		android:layout_marginTop="40dip" android:layout_height="fill_parent">
		<LinearLayout
			android:id="@+id/LinearLayout21"
			android:layout_weight="1"
			android:layout_width="fill_parent"
			android:gravity="center"
			android:layout_height="fill_parent"
		>
			<ImageView
				android:id="@+id/mamoButton"
				android:clickable="true"
				android:src="@drawable/touch_b_stateful" android:layout_height="fill_parent" android:layout_width="fill_parent" android:layout_marginLeft="70dip" android:layout_marginBottom="10dip" android:layout_marginTop="10dip">
			</ImageView>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/LinearLayout22"
			android:layout_weight="1"
			android:gravity="center"
			android:layout_height="fill_parent"
			android:layout_width="fill_parent"
		>
			<ImageView
				android:id="@+id/listButton"
				android:clickable="true"
				android:src="@drawable/touch_b_stateful" android:layout_height="fill_parent" android:layout_width="fill_parent" android:layout_marginRight="70dp" android:layout_marginBottom="10dip" android:layout_marginTop="10dip"></ImageView>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/aboutButtonLayout"
		android:layout_alignParentBottom="true"
		android:gravity="right" android:layout_width="wrap_content" android:layout_alignParentRight="true" android:layout_height="wrap_content">
		<ImageView
			android:clickable="true"
			android:id="@+id/aboutButton"
			android:src="@drawable/touch_s_b_stateful"
			android:layout_height="50dip" android:layout_width="160dp"></ImageView>
	</LinearLayout>
	
	</RelativeLayout>
	